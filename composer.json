{
    "name": "uipps/generate-models-4-packagist",
    "description": "Auto generate models for Laravel Framework code generation.",
    "keywords": ["auto-generate", "model", "laravel"],
    "homepage": "https://uipps.github.io/",
    "support": {
        "issues": "https://github.com/uipps/generate-models-4-packagist/issues",
        "source": "https://github.com/uipps/generate-models-4-packagist"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "uipps",
            "email": "uipps_com@163.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/dbal": ">=2.5",
        "illuminate/support": ">=5.1",
        "illuminate/database": ">=5.1",
        "illuminate/contracts": ">=5.1",
        "illuminate/filesystem": ">=5.1",
        "illuminate/console": ">=5.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": ">=1.4"
    },
    "autoload": {
        "psr-4": {
            "Uipps\\GenerateModels4Packagist\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uipps\\GenerateModels4Packagist\\Coders\\CodersServiceProvider"
            ]
        }
    }
}
